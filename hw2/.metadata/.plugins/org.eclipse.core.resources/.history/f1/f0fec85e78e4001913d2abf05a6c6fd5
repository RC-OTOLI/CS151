package stackMac;

public class Push<T> implements Command {
	T arg;
	Double val;
	
	public Push(T arg) {
		super();
		this.arg = arg;
	}
	
	@Override
	public void execute() throws SyntaxError{
		try {
			val = (Double)arg;
		}
		catch (Exception e) {
			throw new SyntaxError(arg + " cannot be cast to a Double!");
		}
	}

}
