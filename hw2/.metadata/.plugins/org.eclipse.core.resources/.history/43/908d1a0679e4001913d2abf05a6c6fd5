package stackMac;
import java.io.IOException;

public class Push<T> implements Command {
	Double arg;
	
	public Push(T arg) {
		super();
		this.arg = (Double)arg;
	}
	
	@Override
	public void execute() throws SyntaxError{
		try {
			val = (Double)arg;
		}
		catch (IOException e) {
			throw new SyntaxError(arg + " cannot be cast to a Double!");
		}
	}

}
